  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.31.0",
    "allure-commandline": "^2.18.1",
    "chai": "^4.3.4",
    "cypress": "^10.9.0",
    "jest-allure": "^0.1.3",
    "mocha": "^8.4.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "supertest": "^6.1.3"
  }
}
